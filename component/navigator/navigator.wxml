<!--component/navigator/navigator.wxml-->
<view class='navigator'>
  <van-tabbar active="{{ active }}" active-color="{{ChooseColor}}" inactive-color="{{UnChooseColor}}" bind:change="onChangePage" safe-area-inset-bottom="true">
    <van-tabbar-item name='index' icon="wap-home" active>主页</van-tabbar-item>
    <van-tabbar-item name='schedule' icon="todo-list">日程</van-tabbar-item>
    <van-tabbar-item name='focus'>
      <button class='start' style="background-color: {{ChooseColor}};">
      <view>开始</view>
      <view>专注</view>
      </button>
    </van-tabbar-item>
    <van-tabbar-item name='notebook' icon="column">笔记</van-tabbar-item>
    <van-tabbar-item name='mine' icon="manager">我的</van-tabbar-item>
  </van-tabbar>
</view>
<view class='placeholder'></view>